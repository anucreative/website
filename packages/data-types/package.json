{
  "name": "@website/data-types",
  "version": "1.0.0",
  "description": "TypeScript type definitions for CV data",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cv.json": "./dist/cv.json"
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly && cp cv.json dist/cv.json",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
