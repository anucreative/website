{
  "name": "@monorepo/msw",
  "version": "1.0.0",
  "description": "Mock Service Worker setup for API mocking in development",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./handlers": {
      "import": "./dist/handlers.js",
      "types": "./dist/handlers.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "msw": "^2.0.0"
  },
  "devDependencies": {
    "@monorepo/data-types": "workspace:*",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "msw": "^2.0.0"
  }
}
